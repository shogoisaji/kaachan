{
    "cli": {
        "version": ">= 6.0.0",
        "appVersionSource": "local",
        "promptToConfigurePushNotifications": false
    },
    "build": {
        "development": {
            "distribution": "internal",
            "android": {
                "gradleCommand": ":app:assembleDebug"
            },
            "ios": {
                "buildConfiguration": "Release"
            }
        },
        "preview": {
            "distribution": "internal"
        },
        "production": {
            "autoIncrement": true
        }
    },
    "submit": {}
}
